<div class="row">
	<div class="col s12 m8 offset-m2 l6 offset-l3">
		<ul class="collapsible" data-collapsible="accordion">
			<% @place.each do |place| %>
			    <li>
			      <div class="collapsible-header"><%= "#{place.name.capitalize}" %> <span id="place_type"><%= "#{place.type}" %></span></div>
			      <div class="collapsible-body"><h6><%= "#{place.address}" %></h6></div>
			    </li>
			<% end %>
		</ul>
	</div>
</div>
  