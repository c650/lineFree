<div class="container">
    <div class="row">
    <div class="col s6">
        <% @posts.reverse.each do |post| %>
            <div class="card-panel">
                <div class="green-text row">
                    <%="#{post.place.name}" %>
                </div>
                <div class="content row">
                    <%="#{post.wait_time}" %>
                </div>
            </div>
            <% end %>
    </div>
    <div class="col s6">
        <% @place.reverse.each do |place| %>
            <div class="card-panel">
                <div class="green-text row">
                    <%="#{place.place.name}" %>
                </div>
                <div class="content row">
                    <%="#{place.wait_time}" %>
                </div>
            </div>
            <% end %>
    </div>
    </div>
</div>


